{
  "name": "gatsby-starter-rocket-docs",
  "private": true,
  "version": "1.0.0",
  "description": "Website for https://napi.rs",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "starter-name": "gatsby-starter-rocket-docs",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@rocketseat/gatsby-theme-docs": "^2.3.1",
    "gatsby": "^3.11.1",
    "gatsby-plugin-canonical-urls": "^3.11.0",
    "gatsby-plugin-google-gtag": "^3.11.0",
    "gatsby-plugin-manifest": "^3.11.0",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.11.0",
    "gatsby-plugin-sitemap": "^4.7.0",
    "gatsby-transformer-remark": "^4.8.0",
    "prismjs": "^1.24.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/styled": "^11.3.0",
    "@gatsbyjs/reach-router": "^1.3.6",
    "@mdx-js/react": "^1.6.22",
    "@rocketseat/gatsby-theme-docs-core": "^2.3.1",
    "gatsby-link": "^3.11.0",
    "gatsby-plugin-mdx": "^2.11.0",
    "gatsby-react-router-scroll": "^4.11.0",
    "husky": "7",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "mdx-utils": "^0.2.0",
    "mitt": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "parse-numeric-range": "^1.2.0",
    "polished": "^4.1.3",
    "prettier": "^2.3.2",
    "prism-react-renderer": "^1.2.1",
    "react-live": "^2.3.0",
    "vercel": "^23.1.1"
  },
  "scripts": {
    "build": "rm -rf .cache && gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "run-p format:md format:json format:yaml format:source",
    "format:md": "prettier --parser markdown --write './**/*.md'",
    "format:json": "prettier --parser json --write './**/*.json'",
    "format:source": "prettier --config ./package.json --write './**/*.{js,ts}'",
    "format:yaml": "prettier --parser yaml --write './**/*.{yml,yaml}'",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.@(js|jsx)": ["prettier --write"],
    "*.@(yml|yaml)": ["prettier --parser yaml --write"],
    "*.json": ["prettier --parser json --write"],
    "*.@(md|mdx)": ["prettier --parser markdown --write"]
  }
}
