{
  "name": "gatsby-starter-rocket-docs",
  "private": true,
  "version": "1.0.0",
  "description": "Website for https://napi.rs",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "starter-name": "gatsby-starter-rocket-docs",
  "dependencies": {
    "@rocketseat/gatsby-theme-docs": "^2.2.4",
    "gatsby": "^2.31.1",
    "gatsby-plugin-canonical-urls": "^2.9.0",
    "gatsby-plugin-google-gtag": "^2.7.0",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-offline": "^3.9.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.9.0",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-transformer-remark": "^2.15.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "vercel": "^21.1.0"
  },
  "scripts": {
    "build": "rm -rf .cache && gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "run-p format:md format:json format:yaml format:source",
    "format:md": "prettier --parser markdown --write './**/*.md'",
    "format:json": "prettier --parser json --write './**/*.json'",
    "format:source": "prettier --config ./package.json --write './**/*.{js,ts}'",
    "format:yaml": "prettier --parser yaml --write './**/*.{yml,yaml}'"
  },
  "lint-staged": {
    "*.@(js|jsx)": ["prettier --write"],
    "*.@(yml|yaml)": ["prettier --parser yaml --write"],
    "*.json": ["prettier --parser json --write"],
    "*.@(md|mdx)": ["prettier --parser markdown --write"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
